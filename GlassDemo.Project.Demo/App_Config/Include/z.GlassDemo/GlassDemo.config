    <services>
      <configurator type="GlassDemo.Project.Demo.GlassDemoConfigurator, GlassDemo.Project.Demo"/>
      <configurator type="GlassDemo.Project.Demo.GlassMapperConfigurator, GlassDemo.Project.Demo"/>
    </services>
    <pipelines>
      <httpRequestBegin>
        <processor type="GlassDemo.Project.Demo.Pipeline.NotFoundProcessor, GlassDemo.Project.Demo" patch:after="processor[@type='Sitecore.Pipelines.HttpRequest.ItemResolver, Sitecore.Kernel']" />
      </httpRequestBegin>
    </pipelines>
  </sitecore>
</configuration>